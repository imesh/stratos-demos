{
   "type":"lb",
   "provider":"lb",
   "host":"stratos.org",
   "displayName":"Stratos Load Balancer",
   "description":"LB Cartridge",
   "version":"4.0",
   "defaultAutoscalingPolicy":"autoscale-policy1",
   "multiTenant":"false",
   "portMapping":[
      {
         "protocol":"http",
         "port":"80",
         "proxyPort":"80"
      },
      {
         "protocol":"https",
         "port":"443",
         "proxyPort":"443"
      }
   ],
   "deployment":{

   },
   "iaasProvider":[
      {
         "type":"ec2",
         "imageId":"us-east-1/ami-56b1433e",
         "maxInstanceLimit":"1",
         "property":[
            {
               "name":"instanceType",
               "value":"m1.medium"
            },
            {
               "name":"keyPair",
               "value":"stratos-test"
            }
         ]
      }
   ],
   "loadBalancer":{

   },
   "property":[
      {
         "name":"load.balancer",
         "value":"true"
      }
   ]
}